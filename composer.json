{
  "name": "enjoys/forms",
  "description": "forms 5.x dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Enjoys",
      "email": "deadly.info@gmail.com"
    }
  ],
  "require": {
    "php": ">= 8.0",
    "ext-json": "*",
    "ext-gd": "*",
    "guzzlehttp/guzzle": "^7.1",
    "enjoys/traits": "^1.1",
    "enjoys/session": "^2.0",
    "enjoys/server-request-wrapper": "^1.0",
    "httpsoft/http-server-request": "^1.0",
    "enjoys/functions": "^2.1",
    "gabrielelana/byte-units": "0.5.0",
    "webmozart/assert": "^1.10"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^4.7",
    "squizlabs/php_codesniffer": "^3.6",
    "symfony/var-dumper": "^v6.0"
  },
  "scripts": {
    "psalm": "./vendor/bin/psalm",
    "psalm-ext": "./vendor/bin/psalm --show-info=true",
    "infection": [
      "Composer\\Config::disableProcessTimeout",
      "XDEBUG_MODE=coverage ./infection.phar --only-covered --show-mutations --logger-html='mutation-report.html'"
    ]
  },
  "autoload": {
    "psr-4": {
      "Enjoys\\Forms\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Enjoys\\Forms\\": "tests/"
    }
  }
}
